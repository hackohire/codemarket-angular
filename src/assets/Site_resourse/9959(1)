/* SHOWCASE CORE LIBRARY */
showcaseQuery = jQuery.noConflict(true);
showcaseHandlebars = Handlebars;
Handlebars.noConflict();

(function($) {
  $(function() {
		var Handlebars = showcaseHandlebars;

		var Templates = [];

    var Unveil = (function(){
      /**
       * jQuery Unveil (slightly modified)
       * A very lightweight jQuery plugin to lazy load images
       * http://luis-almeida.github.com/unveil
       *
       * Licensed under the MIT license.
       * Copyright 2013 Luís Almeida
       * https://github.com/luis-almeida
       */

      $.fn.unveil = function(threshold, callback) {

        var $w = $(window),
            th = threshold || 0,
            attrib = "data-src",
            media = this,
            loaded;

        media = media.filter(function(){
          return !this.unveiled;
        });

        this.one("unveil", function() {
          var source = this.getAttribute(attrib);
          source = source || this.getAttribute("data-src");
          if (source) {
            this.setAttribute("src", source);
            this.unveiled = true;
            if (typeof callback === "function") callback.call(this);
          }
        });

        function unveil() {
          var inview = media.filter(function() {
            var $e = $(this);
            if ($e.is(":hidden")) return;

            var wt = $w.scrollTop(),
                wb = wt + $w.height(),
                et = $e.offset().top,
                eb = et + $e.height();

            return eb >= wt - th && et <= wb + th;
          });

          loaded = inview.trigger("unveil");
          media = media.not(loaded);
        }

        $w.on("scroll.unveil resize.unveil lookup.unveil", unveil);

        unveil();

        return this;
      };
    })();

		var Helpers = (function() {
			if ((typeof Shopify) === 'undefined') { Shopify = {}; }

			// Set defaults currency settings
			var shop = {};
			shop.currency = 'USD';
			shop.money_format = '${{amount}}';

			function addStyles(href) {
				if (!$('link[href="' + href + '"]').length) {
					var stylesheet = $('<link></link>');

					stylesheet.attr('rel', 'stylesheet');
					stylesheet.attr('href', href);
					stylesheet.appendTo('head');
				}
			}

			function addScript(src) {
				if (!$('script[src="' + src + '"]').length) {
					var script = $('<script></script>');

					script.attr('src', src);
					script.appendTo('body');
				}
			}

			function reloadStyles(styles) {
				var params, reload;

				params = {};
				reload = false;

				// Collect only stylesheet values
				for (var key in styles) {
					if (key.lastIndexOf('styles_', 0) === 0) {
						params[key] = styles[key];
						reload = true;
					}
				}

				// Reload stylesheet if applicable
				if (reload) {
					var stylesheet, href, query;

					stylesheet = $('link[href="https://showcase.abovemarket.com/embed/stylesheet/9959?v2.0.0"]');
					href = stylesheet.attr('href').split('?')[0];
					query = $.param({ preview: params });

					stylesheet.attr('href', href + '?' + query);
				}
			}

			function loadTemplates() {
				var templates, html;

				templates = [];
				html = $.parseHTML("<!-- GALLERY TEMPLATES -->\n<script id=\"showcase-gallery-grid-template\" type=\"text\/template\">\n\t{{#if (displayGalleryTitle) }}\n\t\t<h1 class=\"showcase-gallery-title\">{{{ title }}}<\/h1>\n\t{{\/if}}\n\n\t{{#if (displayGalleryDescription) }}\n\t\t<p class=\"showcase-gallery-description\">{{{ description }}}<\/p>\n\t{{\/if}}\n\n\t<div class=\"showcase-grid\">\n\t\t<div class=\"showcase-grid-items\">\n\t\t\t<!-- Post dynamically loaded here -->\n\t\t<\/div>\n\t\t<div class=\"showcase-grid-buttons\">\n\t\t\t<button class=\"showcase-button load-more\">\n\t\t\t\t<span>{{{ settings.language_gallery_more }}}<\/span>\n\t\t\t\t<div class=\"loading-dots\">\n\t\t\t\t\t<div class=\"loading-dot dot-1\"><\/div>\n\t\t\t\t\t<div class=\"loading-dot dot-2\"><\/div>\n\t\t\t\t\t<div class=\"loading-dot dot-3\"><\/div>\n\t\t\t\t<\/div>\n\t\t\t<\/button>\n\t\t<\/div>\n\t<\/div>\n<\/script>\n\n<script id=\"showcase-gallery-slider-template\" type=\"text\/template\">\n\t{{#if (displayGalleryTitle) }}\n\t\t<h1 class=\"showcase-gallery-title\">{{{title}}}<\/h1>\n\t{{\/if}}\n\n\t{{#if (displayGalleryDescription) }}\n\t\t<p class=\"showcase-gallery-description\">{{{description}}}<\/p>\n\t{{\/if}}\n\n\t<div class=\"showcase-slider\">\n\t\t<div class=\"showcase-track\">\n\t\t\t<div class=\"showcase-grid\">\n\t\t\t\t<div class=\"showcase-grid-items\">\n\t\t\t\t\t<!-- Post dynamically loaded here -->\n\t\t\t\t<\/div>\n\t\t\t<\/div>\n\t\t<\/div>\n\t\t<div class=\"showcase-slider-control prev\"><i class=\"fa fa-chevron-left\"><\/i><\/div>\n\t\t<div class=\"showcase-slider-control next\"><i class=\"fa fa-chevron-right\"><\/i><\/div>\n\t<\/div>\n<\/script>\n\n<script id=\"showcase-gallery-collage-template\" type=\"text\/template\">\n\t{{#if (displayGalleryTitle) }}\n\t\t<h1 class=\"showcase-gallery-title\">{{{ title }}}<\/h1>\n\t{{\/if}}\n\n\t{{#if (displayGalleryDescription) }}\n\t\t<p class=\"showcase-gallery-description\">{{{ description }}}<\/p>\n\t{{\/if}}\n\n\t<div